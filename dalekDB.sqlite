#schema

CREATE TABLE SN_PARAM(
    ATTRIBUTE_ID INTEGER PRIMARY KEY
    ATTRIBUTE_NAME TEXT
    ATTRIBUTE_VALUE TEXT
    ATTRIBUTE_VALUE_TYPE TEXT
)

CREATE TABLE SN_SPECTRA(
    SPECTRUM_ID INTEGER PRIMARY KEY,
    SPECTRUM_DATE TEXT,
    SPECTRUM BLOB
)

CREATE TABLE GA_RUN_PARAMS(
    GA_RUN_ID INTEGER PRIMARY KEY,
    GA_RUN_DESCRIPTION TEXT
)

CREATE TABLE GA_INDIVIDUALS(
    GA_INDIVIDUAL_ID INTEGER PRIMARY KEY
    
)